	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "VSCode for AI Configuration",
			"properties": {
				"ai.telemetry.enable": {
					"type": "boolean",
					"default": true,
					"description": "Enable telemetry for VSCode for AI."
				},
				"ai.submission.always-open-jobproperties": {
					"type": "boolean",
					"default": false,
					"description": "Always open job properties while submission."
				},
				"ai.azureml.enable": {
					"type": "boolean",
					"default": true,
					"description": "Enable Azure ML platform working with AmlWorkbench"
				},
				"ai.azureml.exclusive-mode": {
					"type": "boolean",
					"default": false,
					"description": "Only expose Azure ML platform when Azure ML workbench installed. If Azure ML workbench is not installed, this configuration will not take effort"
				},
				"ai.azureml.login-timeout": {
					"type": "number",
					"default": 300,
					"description": "Azure Account's login timeout. (in seconds)"
				},
				"ai.azureml.workbench-path": {
					"type": "string",
					"default": "",
					"description": "AzureML workbench's installation path"
				}
			}
		},
		"views": {
			"explorer": [
				{
					"id": "AIConfigExplorer",
