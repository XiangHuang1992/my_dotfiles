					"command": "vscodeai.azureml.workspace.add",
					"when": "view == AzureMLExplorer",
					"group": "navigation@2"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer",
					"group": "navigation@3"
				}
			],
			"view/item/context": [
				{
					"command": "vscodeai.selectSubscriptions",
					"when": "view == AzureMLExplorer && viewItem == AzureMLSubscription",
					"group": "inline"
				},
				{
					"command": "vscodeai.addConfiguration",
					"when": "view == AIConfigExplorer && viewItem == BasePlatform"
				},
				{
					"command": "vscodeai.editConfiguration",
					"when": "view == AIConfigExplorer && viewItem == BaseConfig",
					"group": "config@1"
				},
				{
					"command": "vscodeai.removeConfiguration",
					"when": "view == AIConfigExplorer && viewItem == BaseConfig",
					"group": "config@2"
