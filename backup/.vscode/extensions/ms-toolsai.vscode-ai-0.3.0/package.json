			"explorer/context": [
				{
					"command": "vscodeai.menu.viewInJupyter",
					"when": "resourceLangId == jupyter",
					"group": "navigation"
				},
				{
					"command": "vscodeai.menu.submitJob",
					"when": "resourceLangId == cntk"
				},
				{
					"command": "vscodeai.menu.submitJob",
					"when": "resourceLangId == python"
				}
			],
			"editor/context": [
