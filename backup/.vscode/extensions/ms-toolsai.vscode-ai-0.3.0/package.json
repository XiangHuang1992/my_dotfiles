				{
					"command": "vscodeai.feedback.issue",
					"when": "view == AIConfigExplorer",
					"group": "navigation@1"
				},
				{
					"command": "vscodeai.view.common.refresh",
					"when": "view == AIConfigExplorer",
					"group": "navigation@2"
				},
