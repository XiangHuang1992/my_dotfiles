{
	"name": "vscode-ai",
	"displayName": "Visual Studio Code Tools for AI",
	"description": "Build, test, and deploy Deep Learning / AI solutions.",
	"version": "0.3.0",
	"publisher": "ms-toolsai",
	"engines": {
		"vscode": "^1.23.0"
	},
	"icon": "icons/logo.png",
	"preview": true,
	"categories": [
		"Azure",
		"Other"
	],
	"keywords": [
		"Azure Machine Learning",
		"AML",
		"Azure ML",
		"Deep Learning"
	],
	"repository": {
		"url": "https://github.com/Microsoft/vscode-tools-for-ai"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-tools-for-ai/issues",
		"email": "omscs@microsoft.com"
	},
	"extensionDependencies": [
		"ms-vscode.azure-account",
		"ms-python.python"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "%configuration.title%",
			"properties": {
				"ai.homepage.openOnStartup": {
					"description": "%ai.homepage.openOnStartup.description%",
					"type": "boolean",
					"default": true
				},
				"ai.logger.level.file": {
					"desicription": "the level of message in log file",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "debug"
				},
				"ai.logger.level.channel": {
					"desicription": "the level of message in vscode output channel",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "verbose"
				},
				"ai.logger.level.window": {
					"desicription": "the level of message in vscode message window",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "info"
				},
				"ai.telemetry.enable": {
					"type": "boolean",
					"default": true,
					"description": "%ai.telemetry.enable.description%"
				},
				"ai.submission.alwaysOpenJobProperties": {
					"type": "boolean",
					"default": true,
					"description": "%ai.submission.alwaysOpenJobProperties.description%"
				},
				"ai.azureml.enable": {
					"type": "boolean",
					"default": true,
					"description": "%ai.azureml.enable.description%"
				},
				"ai.azureml.exclusiveMode": {
					"type": "boolean",
					"default": false,
					"description": "%ai.azureml.exclusiveMode.description%"
				},
				"ai.azureml.showProgress": {
					"type": "boolean",
					"default": false,
					"description": "%ai.azureml.showProgress.description%"
				},
				"ai.webserver.startPort": {
					"type": "number",
					"default": 21000,
					"description": "%ai.webserver.startPort.description%"
				},
				"ai.tensorboard.startPort": {
					"type": "number",
					"default": 6006,
					"description": "%ai.tensorboard.startPort.description%"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "azure",
					"title": "Azure",
					"icon": "icons/azure.svg",
					"when": "!azuremlDisable"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "AIConfigExplorer",
					"name": "%explorer.aiConfigExplorer.name%",
					"when": "!azuremlExclusive"
				},
				{
					"id": "AIStorageExplorer",
					"name": "%explorer.aiStorageExplorer.name%",
					"when": "enableStorageExplorer"
				}
			],
			"azure": [
				{
					"id": "AzureMLExplorer",
					"name": "Machine Learning",
					"when": "!azuremlDisable"
				}
			]
		},
