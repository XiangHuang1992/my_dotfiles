{
	"name": "vscode-djaneiro",
	"displayName": "Djaneiro - Django Snippets",
	"description": "A collection of snippets for django templates, models, views, fields & forms. Ported from Djaneiro for SublimeText",
	"version": "1.4.0",
	"publisher": "thebarkman",
	"icon": "images/djaneiro.png",
	"galleryBanner": {
		"color": "#0C4B33",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"snippets",
		"python",
		"django"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ScottBarkman/vscode-djaneiro"
	},
	"contributes": {
		"languages": [
			{
				"id": "html",
				"aliases": [
					"Django Template",
					"HTML"
				],
				"extensions": [
					".html",
					".htm"
				],
				"configuration": "./django-html.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "html",
				"scopeName": "text.html.django",
				"path": "./syntaxes/django-html.json"
			}
		],
		"snippets": [
			{
				"language": "python",
				"path": "./snippets/admin/classes.json"
			},
			{
				"language": "python",
